    "lodash": "4.17.21",
    "mousetrap": "chabou/mousetrap#useCapture",
    "ms": "2.1.3",
    "open": "8.4.2",
    "ora": "8.0.1",
    "php-escape-shell": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-use": "^17.5.0",
    "redux": "4.2.1",
